import time

class TetrisGame(object):
    running = True
    
    def run(self):
        print("Hello, World!")
        lastTime = time.time_ns()
        unprocessed = 0
        nsPerTick = 1000000000.0 / 60
        frames = 0
        lastTimer1 = 0
        
        while (running):
            break
        

if __name__ == '__main__':
    app = TetrisGame()
    app.run()